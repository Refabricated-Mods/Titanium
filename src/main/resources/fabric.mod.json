{
  "schemaVersion": 1,
  "id": "titanium",
  "version": "${version}",
  "name": "Titanium",
  "description": "A lib for portality and industrial foregoing",
  "authors": [
    "Trinsdar",
    "TheCodedOne",
    "Buuz135"
  ],
  "contact": {
    "sources": "https://github.com/Refabricated-Mods/Titanium/tree/1.18-fabric"
  },
  "license": "LGPL-3.0-only",
  "environment": "*",
  "accessWidener": "titanium.accesswidener",
  "mixins": [
    "titanium.mixins.json"
  ],
  "entrypoints": {
    "main": [
      "com.hrznstudio.titanium.Titanium"
    ],
    "client": [
      "com.hrznstudio.titanium.TitaniumClient"
    ],
    "fabric-datagen" : [
      "com.hrznstudio.titanium.datagenerator.TitaniumDataGenerator"
    ],
    "modmenu" : [
      "com.hrznstudio.titanium.client.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric" : "*"
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  }
}
